<UserControl x:Class="ScreenCaptureMVVM.View.FinishedControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:viewModel="clr-namespace:ScreenCaptureMVVM.ViewModel" mc:Ignorable="d"
             d:DataContext="{d:DesignInstance viewModel:FinishedViewModel}">

  <UserControl.DataContext>
    <viewModel:FinishedViewModel />
  </UserControl.DataContext>

  <StackPanel Height="300" Width="300">
    <Label Content="EASY SCREEN CAPTURE RESULTS" FontWeight="Bold" FontSize="14" Foreground="#FF586E75" />
    <StackPanel Orientation="Horizontal">
      <Label Content="RESULT:" FontWeight="Bold" FontSize="14" Foreground="#FF586E75" />
      <Label Content="Obtained screen captures." FontSize="14" Foreground="#657b83" />
    </StackPanel>
  </StackPanel>
</UserControl>